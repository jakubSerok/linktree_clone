datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id String @id @default(cuid())
  username String @unique
  email String @unique
  bio String?
  password String
  createdAt DateTime @default(now())
  links Link[]
}

model Link{
  id String @id @default(cuid())
  title String
  url String
  userId String
  clicks Int @default(0)
  user User @relation(fields: [userId], references: [id])
  createdAt DateTime @default(now())
}